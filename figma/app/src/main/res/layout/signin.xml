<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="390dp"
	android:layout_height="wrap_content"
	tools:context=".signin">


	<LinearLayout android:gravity="center_horizontal"
		android:id="@+id/linearSignIn"
		android:layout_height="match_parent"
		android:layout_width="match_parent" android:orientation="vertical">
		<FrameLayout android:gravity="start"
			android:id="@+id/frameSignin"
			android:layout_height="match_parent"
			android:layout_width="match_parent" style="@style/groupStylewhite_A700cornerRadius">
			<ImageView android:id="@+id/imageEllipseFour"
				android:layout_gravity="top|start"
				android:layout_height="@dimen/_402pxv"
				android:layout_marginBottom="@dimen/_10pxv"
				android:layout_width="match_parent"
				android:scaleType="fitXY"
				android:background="@drawable/img_ellipse4"
				tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4"/>
			<FrameLayout android:id="@+id/frameStackellipsefive"
				android:layout_gravity="bottom|start"
				android:layout_height="@dimen/_655pxv"
				android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
				<ImageView android:id="@+id/imageEllipseFive"
					android:layout_gravity="start"
					android:layout_height="match_parent"
					android:layout_width="match_parent"
					android:scaleType="fitXY"
					android:background="@drawable/img_ellipse5"
					tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse5"/>
				<LinearLayout android:id="@+id/linearColumnngnhp"
					android:layout_gravity="bottom|start"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/_10pxv"
					android:layout_width="match_parent" android:orientation="vertical">
					<ImageView android:id="@+id/imageNgnhp"
						android:layout_height="@dimen/_32pxh"
						android:layout_marginEnd="@dimen/_42pxh"
						android:layout_marginStart="@dimen/_42pxh"
						android:layout_width="@dimen/_168pxh"
						android:scaleType="fitXY"
						android:src="@drawable/img_ngnhp"
						tools:ignore="ContentDescription" tools:src="@drawable/img_ngnhp"/>
					<androidx.appcompat.widget.SearchView
						android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_36"
						android:gravity="center_horizontal"
						android:layout_height="wrap_content"
						android:layout_marginEnd="@dimen/_42pxh"
						android:layout_marginStart="@dimen/_42pxh"
						android:layout_marginTop="@dimen/_23pxv"
						android:layout_width="match_parent"
						android:theme="@style/searchViewGroupThree"
						app:iconifiedByDefault="false"
						app:queryBackground="@android:color/transparent"
						app:queryHint="Email hoặc tên đăng nhập"
						app:searchIcon="@drawable/mail"/>
					<androidx.appcompat.widget.SearchView
						android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_36"
						android:gravity="center_horizontal"
						android:id="@+id/searchViewGroupThree"
						android:layout_height="wrap_content"
						android:layout_marginEnd="@dimen/_42pxh"
						android:layout_marginStart="@dimen/_42pxh"
						android:layout_marginTop="@dimen/_23pxv"
						android:layout_width="match_parent"
						android:theme="@style/searchViewGroupThree"
						app:iconifiedByDefault="false"
						app:queryBackground="@android:color/transparent"
						app:queryHint="Mật Khẩu"
						app:searchIcon="@drawable/img_search"/>
					<FrameLayout android:id="@+id/frameStackngNhp"
						android:layout_height="@dimen/_286pxv"
						android:layout_marginTop="@dimen/_41pxv" android:layout_width="match_parent">
						<FrameLayout
							android:id="@+id/frameStackellipsesix"
							android:layout_gravity="bottom|start"
							android:layout_height="@dimen/_273pxv"
							android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
							<ImageView android:id="@+id/imageEllipseSix"
								android:layout_gravity="start"
								android:layout_height="@dimen/_273pxh"
								android:layout_width="match_parent"
								android:scaleType="fitXY"
								android:background="@drawable/img_ellipse6"
								tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse6"/>
							<LinearLayout
								android:id="@+id/linearColumnngnhpbng"
								android:layout_gravity="bottom|center"
								android:layout_height="wrap_content"
								android:layout_marginBottom="@dimen/_37pxv"
								android:layout_marginEnd="@dimen/_52pxh"
								android:layout_marginStart="@dimen/_52pxh"
								android:layout_marginTop="@dimen/_37pxv"
								android:layout_width="match_parent" android:orientation="vertical">
								<LinearLayout android:gravity="start"
									android:id="@+id/linearRowngnhpbng"
									android:layout_height="wrap_content"
									android:layout_width="match_parent"
									android:orientation="horizontal" style="@style/groupStylebluegray_100cornerRadius">
									<ImageView
										android:id="@+id/imageImageFour"
										android:layout_height="@dimen/_33pxh"
										android:layout_marginBottom="@dimen/_9pxv"
										android:layout_marginStart="@dimen/_12pxh"
										android:layout_marginTop="@dimen/_7pxv"
										android:layout_width="@dimen/_30pxh"
										android:scaleType="fitXY"
										android:src="@drawable/img_image4"
										tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>

									<TextView
										android:id="@+id/txtNgnhpbng"
										android:layout_width="0dp"
										android:layout_height="wrap_content"
										android:layout_marginStart="@dimen/_19pxh"
										android:layout_marginTop="@dimen/_14pxv"
										android:layout_marginEnd="@dimen/_32pxh"
										android:layout_marginBottom="@dimen/_13pxv"
										android:layout_weight="0.84"
										android:ellipsize="end"
										android:singleLine="true"
										android:text="Đăng nhập bằng Google"
										android:textSize="16dp" />
								</LinearLayout>

								<LinearLayout
									android:id="@+id/linearRow5065062674fac"
									style="@style/groupStyleblue_A400cornerRadius"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginTop="@dimen/_10pxv"
									android:gravity="start"
									android:orientation="horizontal">

									<ImageView
										android:id="@+id/image5065062674fac"
										android:layout_width="@dimen/_38pxh"
										android:layout_height="@dimen/_37pxh"
										android:layout_marginStart="@dimen/_12pxh"
										android:layout_marginTop="@dimen/_8pxv"
										android:layout_marginBottom="@dimen/_5pxv"
										android:scaleType="fitXY"
										android:src="@drawable/img_5065062674fac"
										tools:ignore="ContentDescription"
										tools:src="@drawable/img_5065062674fac" />

									<TextView
										android:id="@+id/txtNgnhpbngOne"
										android:layout_width="0dp"
										android:layout_height="wrap_content"
										android:layout_marginStart="@dimen/_11pxh"
										android:layout_marginTop="@dimen/_16pxv"
										android:layout_marginEnd="@dimen/_32pxh"
										android:layout_marginBottom="@dimen/_13pxv"
										android:layout_weight="0.84"
										android:ellipsize="end"
										android:singleLine="true"
										android:text="Đăng Nhập Bằng Facebook"
										android:textColor="@color/white"
										android:textSize="16dp"
										tools:text="@string/msg_ng_nh_p_b_ng2" />
								</LinearLayout>

								<TextView
									android:id="@+id/txtChactikho"
									android:textColor="@color/white"
									android:textSize="16dp"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginStart="@dimen/_37pxh"
									android:layout_marginTop="@dimen/_27pxv"
									android:layout_marginEnd="@dimen/_37pxh"
									android:ellipsize="end"
									android:singleLine="true"
									android:text="Chưa có tài khoản? Đăng Ký"
									tools:text="@string/msg_ch_a_c_t_i_kho" />
							</LinearLayout>

							<androidx.appcompat.widget.AppCompatButton
								android:id="@+id/btnngNhp"
								style="@style/btnSolidRoundedOutline"
								android:layout_width="190dp"
								android:layout_height="wrap_content"
								android:layout_gravity="top|center"
								android:layout_marginStart="@dimen/_121pxh"
								android:layout_marginEnd="@dimen/_121pxh"
								android:layout_marginBottom="@dimen/_10pxv"
								android:ellipsize="end"
								android:gravity="center"
								android:paddingStart="@dimen/_30pxh"
								android:paddingTop="@dimen/_11pxv"
								android:paddingEnd="@dimen/_30pxh"
								android:paddingBottom="@dimen/_11pxv"
								android:singleLine="true"
								android:text="@string/lbl_ng_nh_p"
								android:textAllCaps="false"
								tools:text="@string/lbl_ng_nh_p" />
						</FrameLayout>

					</FrameLayout>
				</LinearLayout>
			</FrameLayout>
			<ImageView android:id="@+id/imageImageThree"
				android:layout_gravity="top|center"
				android:layout_height="@dimen/_181pxh"
				android:layout_marginBottom="@dimen/_10pxv"
				android:layout_marginEnd="@dimen/_112pxh"
				android:layout_marginStart="@dimen/_112pxh"
				android:layout_width="@dimen/_189pxh"
				android:scaleType="fitXY"
				android:background="@drawable/img_image3"
				tools:ignore="ContentDescription"
				tools:src="@drawable/img_image3"/>
		</FrameLayout>
	</LinearLayout>
</RelativeLayout>