<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameSignup"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylebluegray_800cornerRadius">
            <FrameLayout android:id="@+id/frameStackellipsethree"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_402pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageEllipseThree"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_402pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse3"/>
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_23pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackellipsetwo"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_655pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageEllipseTwo"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse2"/>
                <LinearLayout android:id="@+id/linearColumnngktikho"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtNgktikho"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{signUpVM.signUpModel.txtNgktikho}"
                        style="@style/txtInterbold32" tools:text="@string/msg_ng_k_t_i_kho"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowperson"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_1">
                        <ImageView android:id="@+id/imagePerson"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_28pxv"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_person"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_person"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnlanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_176pxh"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLanguage"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtLanguage}"
                                style="@style/txtInterregular16" tools:text="@string/lbl_t_n_t_i_kho_n"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtHehehehe"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtHehehehe}"
                                style="@style/txtInterbold20" tools:text="@string/lbl_hehehehe"/>
                        </LinearLayout>
                    </LinearLayout>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_37"
                        android:drawablePadding="@dimen/_11pxh"
                        android:drawableStart="@drawable/img_mail"
                        android:ellipsize="end"
                        android:hint="@string/lbl_email"
                        android:id="@+id/etGroupEleven"
                        android:inputType="textEmailAddress"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_23pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_21pxh"
                        android:paddingTop="@dimen/_23pxv"
                        android:singleLine="true"
                        android:text="@={signUpVM.signUpModel.etGroupElevenValue}"
                        android:textColorHint="@color/black_900_7f"
                        style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                    <androidx.appcompat.widget.SearchView
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_37"
                        android:gravity="center_horizontal"
                        android:id="@+id/searchViewGroupNine"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="match_parent"
                        android:theme="@style/searchViewGroupNine"
                        app:iconifiedByDefault="false"
                        app:queryBackground="@android:color/transparent"
                        app:queryHint="@string/lbl_m_t_kh_u" app:searchIcon="@drawable/img_search"/>
                    <FrameLayout android:gravity="start"
                        android:id="@+id/frameStackellipseone"
                        android:layout_height="@dimen/_300pxv"
                        android:layout_marginTop="@dimen/_18pxv" android:layout_width="match_parent">
                        <ImageView android:id="@+id/imageEllipseOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_300pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_ellipse1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1"/>
                        <LinearLayout
                            android:id="@+id/linearColumngroupten"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_42pxh"
                            android:layout_marginStart="@dimen/_42pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <androidx.appcompat.widget.SearchView
                                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_37"
                                android:gravity="start"
                                android:id="@+id/searchViewGroupTen"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:theme="@style/searchViewGroupTen"
                                app:iconifiedByDefault="false"
                                app:queryBackground="@android:color/transparent"
                                app:queryHint="@string/msg_nh_p_l_i_m_t_kh" app:searchIcon="@drawable/img_search"/>

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnngK"
                                style="@style/btnSolidRoundedOutline_1"
                                android:layout_width="@dimen/_172pxh"
                                android:layout_height="@dimen/_48pxv"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginStart="@dimen/_58pxh"
                                android:layout_marginTop="@dimen/_33pxv"
                                android:layout_marginEnd="@dimen/_58pxh"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_11pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingBottom="@dimen/_11pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_ng_k"
                                android:textAllCaps="false"
                                tools:text="@string/lbl_ng_k" />

                            <TextView
                                android:id="@+id/txtCtikhon"
                                style="@style/txtInterregular16_1"
                                android:layout_width="280dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_marginTop="@dimen/_46pxv"
                                android:layout_marginEnd="@dimen/_58pxh"
                                android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtCtikhon}"
                                tools:text="@string/msg_c_t_i_kho_n" />

                            <View android:background="@color/black_900"
                                android:id="@+id/lineLineThirtyFour"
                                android:layout_gravity="end"
                                android:layout_height="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_70pxh"
                                android:layout_marginStart="@dimen/_70pxh"
                                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_59pxh"/>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageImageThree"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_91pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_153pxh"
                android:layout_marginStart="@dimen/_153pxh"
                android:layout_width="@dimen/_98pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image3_91x98}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image3_91x98"/>
        </FrameLayout>
    </LinearLayout>
</layout>
